<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Portfolio{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}}
    </head>
    <body>
    <div class="d-flex vh-100">
        <header class="col-2 pl-0">
            {% include '_components/navbar.html.twig' %}
        </header>
        <main class="col-10">
            {% block body %}{% endblock %}
        </main>
    </div>
    {% block javascripts %}{% endblock %}
    {{ encore_entry_script_tags('app') }}
    </body>
</html>
